def esPrimo(num):
    con= 0
    for x in range(1,num+1):
            if num % x  == 0:
                con = con +1
    return con
     
def lista(ini,fin):
     print("los Numeros Primos en el rango de [%d,%d] son:"%(ini,fin))
     print(" NÂ°  divi3sores")
     for j  in range(ini,fin):
         v = esPrimo(j)
         if v==2:
             print("  %d => %d"%(j,esPrimo(j)))
ini =int(input("Ingrese el rango inicial: "))
fin =int(input("Ingrese el rango final: "))
lista(ini,fin)
